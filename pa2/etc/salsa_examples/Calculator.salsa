module fibonacci;

behavior Calculator {
		
	int fib(int n) { 
		Fibonacci f = new Fibonacci(n);
		f <- compute() @ currentContinuation;
	}
	
	int add(int n1, int n2) {return n1+n2;} 
	
	void act(String args[]) {
		fib(15) @ standardOutput <- println(token);
		fib(5) @ add(token,3) @
		standardOutput <- println(token);
	}
}

